  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#ffc333"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "foreground": "#81ddff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#f7569c"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword.control.conditional", "keyword.control.import"],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#FFF"
      }
    },
    {
      "name": "Punctuation Parameters",
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "#FFF"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "entity.name.variable.parameter",
        "meta.at-rule.function variable",
        "meta.at-rule.mixin variable",
        "meta.function.arguments",
        "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql",
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#e48cff"
      }
    },
    {
      "name": "Punctuation Template Expression",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#6b7de2"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#ff5aaa"
      }
    },
    {
      "name": "Storage Type Arrow Function",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#ff5aaa"
      }
    },
    {
      "name": "String",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#a5f575"
      }
    },
    {
      "name": "String Template",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#a5f575"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#ccffcc"
      }
    },
    {
      "name": "Support Function",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#22bbff"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#fff"
      }
    },
    // Lines
    {
      "name": "Meta Brace",
      "scope": "meta.brace",
      "settings": {
        "foreground": "#888"
      }
    },
    // DART -----------------------------------
    {
      "name": "Dart Primitives",
      "scope": "storage.type.primitive.dart",
      "settings": {
          "foreground": "#ccffcc"
      }
    },
    {
      "name": "String escape characters",
      "scope": "constant.character.escape.dart",
      "settings": {
